<% @songs.each do |song| %>
    <h2><a href=<%= "/songs/#{song.slug}" %>><%= song.name %></a></h2>
    <% if !song.artist.nil? %>
        <h3><%= song.artist.name %></h3>
    <% end %>
    <br>

    <h3>Genres: </h3>
    <% song.genres.each do |genre| %>
        <p><%= genre.name %></p>
    <% end %>

<% end %>